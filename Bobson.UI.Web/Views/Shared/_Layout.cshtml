<!DOCTYPE html>
<html>
<head>
    <link rel="icon" type="image/png" sizes="96x96" href="@Url.Content("~/favicon.png")">
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Bobson Latin America</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/summernotecss")
    @Scripts.Render("~/bundles/modernizr")
    @RenderSection("CustomStyles", required: false)

    <script src="http://ajax.aspnetcdn.com/ajax/jQuery/jquery-1.10.0.min.js" type="text/javascript"></script>
    <script src="http://ajax.aspnetcdn.com/ajax/jquery.ui/1.9.2/jquery-ui.min.js" type="text/javascript"></script>
    <link href="http://ajax.aspnetcdn.com/ajax/jquery.ui/1.9.2/themes/blitzer/jquery-ui.css"
          rel="Stylesheet" type="text/css" />


    <script type="text/javascript" src="http://maps.googleapis.com/maps/api/js?key=AIzaSyAV44R3VYfRJnMKZljwfmDCeVMhb3fegN8&sensor=false">
    </script>

    
</head>
<body>
    <nav id="barraFerramentasSite" class="navbar navbar-default navbar-fixed-top">
        <div class="container-fluid">

            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="/Home/Index">
                    <img alt="Brand" src="~/Images/brand.png">
                </a>
            </div>

            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                @Html.Action("Menu", "Shared")
                @Html.Action("Login", "Shared")
                @*Html.Partial("_LoginPartial")*@
            </div>
        </div>
    </nav>

    <div class="clearfix"></div>

    @Html.MvcSiteMap().SiteMapPath()

    <div class="container body-content">
        @RenderBody()
    </div>

    @Html.Partial("_FooterPartial")


    
    @Scripts.Render("~/bundles/bootstrap")

    <script type="text/JavaScript">

        $(document).ready(function () {

        });

    </script>

    @RenderSection("scripts", required: false)
    </body>
</html>
